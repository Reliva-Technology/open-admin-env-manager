    "type": "library",
    "keywords": ["laravel-admin", "extension"],
    "homepage": "https://github.com/fadlisaad/ext-env-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "fadlisaad",
            "email": "fadlisaad@gmail.com"
        }
