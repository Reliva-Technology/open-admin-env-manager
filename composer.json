{
    "name": "reliva-technology/open-admin-env-manager",
    "description": "Env Manager for Laravel Open Admin",
    "type": "library",
    "keywords": ["open-admin", "extension", "env"],
    "homepage": "https://github.com/Reliva-Technology/open-admin-env-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "fadlisaad",
            "email": "tech@reliva.com.my"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "open-admin-org/open-admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Reliva\\EnvManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Reliva\\EnvManager\\EnvManagerServiceProvider"
            ]

        }
    }
}
